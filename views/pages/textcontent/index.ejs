<div class="d-flex flex-grow-1 flex-shrink-1">
    <div class="qntm-app-navbar-container flex-fill border-right" role="menubar">
        <ul class="qntm-app-navbar-links" role="menu">
            <div class="d-flex flex-nowrap mb-3">
                <form action="/textcontent">
                    <input type="text" value="" length="20" name="q" placeholder="Search in title" class="w-75">
                    <button type="submit" id="app-search-button-icon" class="shape-rounded-corners-small">
                        <img src="images/icon_search.svg" alt="Search Icon">
                    </button>
                </form>
                <form action="/textcontent/new">
                    <button class="shape-rounded-corners-small">
                        <svg id="app-copy-button-icon" focusable="false" viewBox="0 0 24 24" aria-hidden="true"
                             data-testid="AddIcon" tabindex="-1" title="Add">
                            <path d="M19 13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z"></path>
                        </svg>
                    </button>
                </form>
            </div>
            <% textcontents.forEach(function(entry){ %>
                <li class="qntm-navbar-list-item qntm-app-navbar-list-item">
                    <a href="/textcontent/<%= entry.id %>/edit"
                       class="qntm-navbar-link font-special-text blue-font"><%= entry.title %></a>
                </li>
            <% }); %>
        </ul>
    </div>
    <div class="flex-fill grey-background d-flex flex-row justify-content-center align-items-center">
        <span class="">TODO Welcome text</span>
    </div>
</div>

